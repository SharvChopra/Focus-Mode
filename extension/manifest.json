{
  "manifest_version": 3,
  "name": "Focus Mode Enforcer",
  "version": "1.0",
  "description": "Blocks distracting sites during scheduled focus periods.",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "alarms", "declarativeNetRequest"],
  "host_permissions": ["<all_urls>"],
  "action": {}
}
